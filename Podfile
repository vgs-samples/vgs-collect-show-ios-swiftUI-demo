# Uncomment the next line to define a global platform for your project
# platform :ios, '13.0'

target 'VGSCollectShowSwiftUIDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

	# Collect information securely with VGS.
	# https://github.com/verygoodsecurity/vgs-collect-ios
  pod 'VGSCollectSDK'

	# Reveal and show information securely with VGS.
	# https://github.com/verygoodsecurity/vgs-show-ios
  pod 'VGSShowSDK'

	# VGSCollectSDK + CardIO integration.
	pod 'VGSCollectSDK/CardIO'

	# VGSCollectSDK + CardScan integration.
	pod 'VGSCollectSDK/CardScan'

end
